{
  "version": 2,
  "functions": {
    "api/send-email.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/style\\.css",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/public/style.css"
    },
    {
      "src": "/script\\.js",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/public/script.js"
    },
    {
      "src": "/images/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/public/images/$1"
    },
    {
      "src": "/(.*\\.(?:jpg|jpeg|gif|png|mp4))",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 